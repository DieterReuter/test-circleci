{
  "ArtifactsDest": "./pkg",
  "Tasks": [
    "xc",
    "archive-zip",
    "archive-tar-gz"
  ],
  "TaskSettings": [
    "archive": {
      "os": {
        "linux": "TarGz",
        "darwin": "Zip",
        "windows": "Zip"
      }
    },
  ],  
  "BuildConstraints": "linux darwin windows",
  "ResourcesInclude": "$name$.*.conf",
  "ConfigVersion": "0.9",
  "Env": [
    "GOPATH={{.Env.PWD}}{{.PS}}Godeps{{.PS}}_workspace{{.PLS}}{{.Env.GOPATH}}"
  ]
}